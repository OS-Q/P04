{
  "name": "P151",
  "title": "OS-Q WCH 8051 CH55X",
  "description": "8051 core && USB device.",
  "homepage": "http://www.wch.cn/products/CH554.html",
  "engines": {
    "platformio": "^5"
  },
  "keywords": [
    "OS-Q",
    "dev-platform",
    "WCH",
    "CH55x",
    "8051"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/P151.git"
  },
  "version": "1.0.0",
  "frameworks": {
    "arduino": {
      "package": "A128",
