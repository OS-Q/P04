      "version": "~1.40100.0"
    },
    "A128": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/A128.git"
    },
    "E128": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/E128.git"
    },
    "tool-openocd": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~2.1000.0"
    }
  }
